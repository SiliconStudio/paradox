<!-- Build file pre-included by all Paradox projects -->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <!--  Defines the API used by Paradox - Values are Direct3D/Null/OpenGL/OpenGLES -->
    <ParadoxGraphicsApi Condition=" '$(ParadoxGraphicsApi)' == '' ">Direct3D11</ParadoxGraphicsApi>
    <SiliconStudioOpenSourceSign Condition="'$(SiliconStudioOpenSourceSign)' == '' And '$(SiliconStudioSign)' != 'true'">true</SiliconStudioOpenSourceSign>
  </PropertyGroup>
</Project>